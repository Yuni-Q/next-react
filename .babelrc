{
	"presets": ["next/babel"],
	"plugins": [
		[
			"babel-plugin-styled-components",
			{
				"ssr": true,
				"displayName": true,
				"fileName": true
				//"preprocess": false
			}
		],
		[
			"@babel/plugin-proposal-decorators",
			{
				"legacy" : true
